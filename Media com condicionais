import java.util.Scanner;
public class Main
{
    public static void main(String[] args) {
            Scanner scan = new Scanner(System.in);
     int n1 = scan.nextInt();
     int n2 = scan.nextInt();
     int n3 = scan.nextInt();
     int n4 = scan.nextInt();
     float media = (n1+n2+n3+n4)/4f;
     
     if(media >= 70){
         System.out.println("Você passou de ano com uma media de "+media);
     }else{
         System.out.println("Você não passou de ano com uma media de "+ media);
     }
    }
}
